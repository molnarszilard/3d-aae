{
    "experiment_name": "experiment",
    "results_root": "datasets/results/",
    "dec_path": "/home/szilard/projects/DisentangledVAE/latent_samples/",
    "clean_results_dir": false,

    "cuda": true,
    "gpu": 3,

    "reconstruction_loss": "chamfer",

    "metrics": [
    ],

    "latent_image_height": 64,
    "latent_image_width": 64,
    "dataset": "shapenet",
    "data_dir": "datasets/shapenet/",
    "classes": [],
    "shuffle": true,
    "transforms": ["rotate"],
    "num_workers": 8,
    "n_points": 2048,

    "max_epochs": 200,
    "batch_size": 50,
    "gp_lambda": 10,
    "reconstruction_coef": 0.05,
    "z_size": 2048,
    "distribution": "bernoulli",

    "p": 0.2,
    "z_beta_a": 0.01,
    "z_beta_b": 0.01,

    "normal_mu": 0.0,
    "normal_std": 0.2,

    "seed": 2018,
    "save_frequency": 5,
    "epsilon": 0.001,

    "arch": "aae",
    "model": {
        "D": {
            "dropout": 0.5,
            "use_bias": true,
            "relu_slope": 0.2
        },
        "G": {
            "use_bias": true,
            "relu_slope": 0.2
        },
        "E": {
            "use_bias": true,
            "relu_slope": 0.2
        }
    },
    "optimizer": {
        "D": {
            "type": "Adam",
            "hyperparams": {
                "lr": 0.0005,
                "weight_decay": 0,
                "betas": [0.9, 0.999],
                "amsgrad": false
            }
        },
        "EG": {
            "type": "Adam",
            "hyperparams": {
                "lr": 0.0005,
                "weight_decay": 0,
                "betas": [0.9, 0.999],
                "amsgrad": false
            }
        }
    }
}
